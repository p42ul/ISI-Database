# Libraries
library(tidyverse)
library(ggthemes)
library(reshape2)
library(plyr)
library(plotly)
# Data importation
all.data <- read.csv("installationsList.csv", header=T, sep=",", na.strings=c("NA", "N/A", "#VALUE!"))
all.data$pubYear <- str_extract(all.data$References, '\\d{4}')
# Data Analysis
all.data$Publication  %>%
table()
all.data$References %>%
table()
# Ploting data
all.data  %>%
select(pubYear) %>%
melt() %>%
ddply(cbind('pubYear'), summarise, sum = table(pubYear)) %>%
ggplot(
aes(
x = pubYear,
y = sum
)
) +
geom_col(fill = "#0F2080") +
theme_gdocs() +
ylab('Number of Publications') +
xlab('Year') +
coord_cartesian(ylim = c(0,21))
